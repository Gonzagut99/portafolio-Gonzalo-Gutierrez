---
import Layout from '../layouts/Layout.astro';
import SectionContainer from '../components/SectionContainer.astro';
import Experience from '../components/Experience.astro';
import BriefCase from '../components/icons/BriefCase.astro';
import ExperienceParallax from '../components/AboutMeParallax/ExperienceParallax';
---
<Layout 
title="Gonzalo Gutiérrez - Experiencia"
description="Contrata a Gonzalo Gutiérrez (@gonza_dvui) Trainee fullstack en desarrollo web y móvil. Diseñador UX/UI. De Arequipa, Perú. Apasionado por crear aplicaciones significativas y desafiantes."
>
	<main class="px-4 mb-auto pt-12 flex flex-col flex-grow">
		<SectionContainer id="experience" class="py-6 pt-14 !pb-4 flex flex-col gap-y-2 sm:gap-y-3 md:gap-y-4 lg:gap-y-5">
			<h2 class="text-gray-200 text-3xl font-semibold mb-6 flex gap-x-2 self-center">
				<BriefCase class="size-8"></BriefCase>
				Experiencia
			</h2>
			<!-- <Experience></Experience> -->
		</SectionContainer>
		<section id="sobre_mi_parallax" class="relative w-full h-full flex flex-col gap-y-2 sm:gap-y-3 md:gap-y-4 lg:gap-y-5 lg:w-[740px] mx-auto pb-8 sm:pb-10 md:pb-14 lg:pb-20">
            <ExperienceParallax client:load id='parallaxExperiencia' ></ExperienceParallax>
        </section>
	</main>
</Layout>